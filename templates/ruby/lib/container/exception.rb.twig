module {{spec.title | caseUcfirst}}
    class Exception < StandardError
        attr_reader :code
        attr_reader :response
        attr_reader :type
        
        def initialize(message, code = 0, type = nil, response = nil)
            super(message)
            @code = code
            @type = type
            @response = response
        end
    end


{% if spec.definitions.appwriteException.errorTypes|length > 0 %}
class ErrorType
{% for error in spec.definitions.appwriteException.errorTypes %}
    # {{ error.message }}
    {{ error.type|title|replace({'_': ''}) }} = '{{ error.type }}'
{% endfor %}
end
{% endif %}
end