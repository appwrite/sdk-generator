{
  "name": "{{ language.params.npmPackage | caseDash }}",
  "homepage": "{{ spec.contactURL }}",
  "description": "{{ sdk.shortDescription }}",
  "version": "{{ sdk.version }}",
  "license": "{{ sdk.license }}",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsup"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "{{ sdk.gitURL }}"
  },
  "devDependencies": {
    "@types/node": "20.11.25",
    "tsup": "7.2.0",
    "esbuild-plugin-file-path-extensions": "^2.0.0",
    "tslib": "2.6.2",
    "typescript": "5.4.2"
  },
  "dependencies": {
    "node-fetch-native-with-agent": "1.7.2",
    "parse-multipart-data": "^1.5.0"
  }
}
